{
	"name": "aimeos-extensions/templates",
	"description": "Aimeos templates extension",
	"keywords": ["aimeos", "extension"],
	"type": "typo3-cms-extension",
	"prefer-stable": true,
	"minimum-stability": "dev",
	"require": {
		"php": "~7.1||^8.0.11"
	},
	"autoload": {
		"classmap": ["Classes"],
		"psr-4": {
			"Aimeos\\Aimeos\\": "Classes"
		}
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "templates"
		}
	}
}
